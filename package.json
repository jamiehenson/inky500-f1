{
  "name": "inky-standings",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && cp -r announcements dist/announcements",
    "preview": "vite preview",
    "build-only": "vite-ssg build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "standings": "tsx src/scripts/generateStandings.ts"
  },
  "dependencies": {
    "@unhead/vue": "^1.8.18",
    "ably": "^2.0.4",
    "flag-icons": "^7.1.0",
    "loaders.css": "^0.1.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "~5.3.0",
    "vite": "^5.1.7",
    "vite-ssg": "^0.23.6",
    "vue-tsc": "^1.8.27"
  }
}
